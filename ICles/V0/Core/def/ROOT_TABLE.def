COMMAND="Select -s from TABLE_INFO where ROOT_TABLE=1 and ACTIVE=1 and MODULE=%Module%"
SEP="§"
FORMAT="ROOT_TABLE§ACTIVE§TABLE_NAME§TYPE_SOURCE§PARAM_SOURCE§MODULE§LABEL_FIELD§DESCRIPTION"
SIZE="2b§2b§20s§20s§20s§20s§20s§20s"
KEY="TABLE_NAME"

FKEY="[MODULE] on MODULE[Module]"
FKEY="[TYPE_SOURCE] on TYPE_SOURCE[Name]"