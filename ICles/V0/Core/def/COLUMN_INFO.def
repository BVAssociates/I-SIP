COMMAND="PC_LIST_COLUMN_INFO.pl -s, %Environnement% %TABLE_NAME%"
SEP=","
FORMAT="TABLE_NAME,FIELD_NAME, TEXT, TYPE, PRIMARY_KEY, FOREIGN_TABLE, FOREIGN_KEY"
SIZE="20s,20s ,20s, 20s, 2b, 20s, 20s"
KEY="TABLE_NAME,FIELD_NAME"

FKEY="[TYPE] on TYPE[Name]"
FKEY="[TABLE_NAME] on TABLE_INFO[TABLE_NAME]"
FKEY="[FOREIGN_TABLE, FOREIGN_KEY] on KEY_INFO[TABLE_NAME, FIELD_NAME]"

