COMMAND="PC_LIST_TABLE.pl -s, %Environnement% COLUMN_INFO"
SEP=","
FORMAT="TABLE_NAME,FIELD_NAME ,DATE_UPDATE, USER_UPDATE, DATA_TYPE, DATA_LENGTH, TEXT, TYPE, PRIMARY_KEY, FOREIGN_TABLE, FOREIGN_KEY"
SIZE="20s,20s ,20s, 20s, 20s, 20s ,20s, 20s, 2b, 20s, 20s"
KEY="TABLE_NAME,FIELD_NAME"

FKEY="[TYPE] on TYPE[Name]"
FKEY="[TABLE_NAME] on TABLE_INFO[TABLE_NAME]"
FKEY="[FOREIGN_TABLE, FOREIGN_KEY] on COLUMN_INFO[TABLE_NAME, FIELD_NAME]"
